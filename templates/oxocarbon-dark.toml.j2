# Primary colors
[colors.primary]
background = "#{{ base00 }}"
foreground = "#{{ base06 }}"

# Search colors
[colors.search.matches]
foreground = CellBackground
background = '#{{ base0A }}'

# Normal colors
[colors.normal]
black = '#{{ base01 }}'
