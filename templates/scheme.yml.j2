OxocarbonDark: &OxocarbonDark
  primary:
    background: "#{{ base00 }}"
    foreground: "#{{ base06 }}"
  search:
    matches:
      foreground: CellBackground
      background: '#{{ base0A }}'
    footer_bar:
      background: '#{{ base01 }}'
      foreground: '#{{ base06 }}'
  normal:
    black: "#{{ base01 }}"
    magenta: "#{{ base0C }}"
    green: "#{{ base0D }}"
    yellow: "#ffe97b"
    blue: "#{{ base0B }}"
    red: "#{{ base0A }}"
    cyan: "#{{ base08 }}"
    white: "#{{ base04 }}"
  bright:
    black: "#{{ base02 }}"
    magenta: "#{{ base0C }}"
    green: "#{{ base0D }}"
    yellow: "#ffe97b"
    blue: "#{{ base0B }}"
    red: "#{{ base0A }}"
    cyan: "#{{ base08 }}"
    white: "#{{ base06 }}"
